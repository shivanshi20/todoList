
<%- include("header") -%>

  <div class="box" id="heading">
    <h1><%= ListTitle %></h1>
  </div>

  <div class="box">
    <% for(var i = 0; i < newlistItems.length ; i++){ %>
    <div class="item">
      <input type="checkbox">
      <p><%= newlistItems[i] %></p>
    </div>
    <%  } %>

    <form class="item" action="<%= route %>" method="post">
      <input type="text" name="newItem" placeholder="NewItems" autocomplete="off">
      <button type="submit" name="List" value="<%= ListTitle %>">+</button>
    </form>
  </div>

  <%- include("footer")-%>
